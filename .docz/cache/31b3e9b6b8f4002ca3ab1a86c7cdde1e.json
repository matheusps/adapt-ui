content */\n  content?:\n    | 'flex-start'\n    | 'flex-end'\n    | 'center'\n    | 'space-between'\n    | 'space-around'\n    | 'stretch'\n}\n\ninterface Props extends Flex {\n  /** Lifting of surface, none => substratum */\n  lifting?: lifting\n}\n\nconst Surface = styled.div<Props>(\n  ({\n    lifting,\n    theme: {\n      colors: { text, surface, substratum },\n    },\n    ...props\n  }) => ({\n    zIndex: lifting ? lifting : 0,\n    backgroundColor: lifting ? surface[lifting] : substratum,\n    color: text.display,\n    margin: '0.5rem',\n    padding: '1rem',\n    borderRadius: '.8rem',\n    width: props.width,\n    height: props.height,\n    order: props.order,\n    flexShrink: props.shrink,\n    flexGrow: props.grow,\n    flexBasis: props.basis,\n    alignSelf: props.self,\n    display: props.inline ? 'inline-flex' : 'flex',\n    flexDirection: props.direction,\n    flexWrap: props.wrap,\n    justifyContent: props.justify,\n    alignItems: props.items,\n    alignContent: props.content,\n  })\n)\n\nSurface.defaultProps = {\n  width: 'auto',\n  height: 'auto',\n  order: 0,\n  shrink: 1,\n  grow: 0,\n  self: 'auto',\n  basis: 'auto',\n  inline: false,\n  direction: 'row',\n  wrap: 'nowrap',\n  justify: 'flex-start',\n  items: 'stretch',\n  content: 'stretch',\n}\n\nexport default Surface\n"]}]}