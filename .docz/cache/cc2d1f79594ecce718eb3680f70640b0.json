e,QAAfA,QAAe;AAClD,MAAMC,CAAC,GAAGJ,YAAY,CAACC,KAAD,CAAtB;AACA,SAAO,oBAAC,CAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAIE,QAAJ,CAAP;AACD,CAHD;;eAKeD,O;AAAf;;;;;;;;;;;;;;;;;;;;;;0BAlDMd,E;0BAKAE,E;0BAIAE,E;0BAIAE,E;0BAIAE,E;0BAIAE,E;0BAKAE,Y;0BAmBAE,O","sourcesContent":["import React, { FC } from 'react'\nimport styled from 'styled-components'\n\ntype HLevel = 1 | 2 | 3 | 4 | 5 | 6\n\ninterface Props {\n  level: HLevel\n}\n\nconst H1 = styled.h1`\n  font-size: 4rem;\n  font-weight: 800;\n  margin: 0.67em 0;\n`\nconst H2 = styled.h2`\n  font-size: 3.5rem;\n  font-weight: 800;\n`\nconst H3 = styled.h3`\n  font-size: 3rem;\n  font-weight: 600;\n`\nconst H4 = styled.h4`\n  font-size: 2.5rem;\n  font-weight: 600;\n`\nconst H5 = styled.h5`\n  font-size: 2rem;\n  font-weight: 600;\n`\nconst H6 = styled.h6`\n  font-size: 1.5rem;\n  font-weight: 600;\n`\n\nconst getComponent = (level: HLevel) => {\n  switch (level) {\n    case 1:\n      return H1\n    case 2:\n      return H2\n    case 3:\n      return H3\n    case 4:\n      return H4\n    case 5:\n      return H5\n    case 6:\n      return H6\n    default:\n      return H1\n  }\n}\n\nconst Heading: FC<Props> = ({ level, children }) => {\n  const H = getComponent(level)\n  return <H>{children}</H>\n}\n\nexport default Heading\n"]}]}